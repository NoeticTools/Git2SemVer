{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/material",
      "templates/my-template"
    ],
    "globalMetadata": {
      "_appName": "Git2SemVer",
      "_appTitle": "Git2SemVer",
      "_appLogoPath": "Images/Git2SemVer_icon_32x32.png",
      "_enableSearch": true,
      "pdf": false
    },
    "markdownEngineProperties": {
      "markdigExtensions": ["Diagrams"]
    }
  }
}